#!/sbin/sh

###########################################WARNING############################################## 
##                                                                                            ##
## NO PERSONAL USE WITHOUT PERMISSION OF SkywalkerZ                                           ##
## ANY OTHER USE WITHOUT PERMISSION WILL BE REPORTED                                          ##
##                                                                                            ##
## YOU HAVE BEEN WARNED                                                                       ##
##                                                                                            ##
## PART OF SMARTDROID                                                                         ##
##                                                                                            ##
################################################################################################

mount /dev/block/mmcblk0p23 /system
mount /dev/block/mmcblk0p26 /data

cd /system/app
rm /system/app/Dropbox.apk
rm /system/app/Dropbox.odex
rm /system/app/DropboxOOBE.apk
rm /system/app/DropboxOOBE.odex
cd /system/lib
cd /data/app
rm /data/app/com.dropbox.android-1.apk

rm -r /data/dalvik-cache

exit

##By SkywalkerZ
## For Any Roms G900F KK